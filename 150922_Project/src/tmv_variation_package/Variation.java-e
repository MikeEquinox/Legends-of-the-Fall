package tmv_variation_package;

/**
 *  @author teleginmv
 *  this example shows inheritance VARIATION
 *  AnalogClock and its inheritance DigitalClock
 */

public class Variation {

	public static void main(String[] args) {
		
		AnalogClock ac = new AnalogClock();
		DigitalClock dc = new DigitalClock();
		ac.calcTime();
		ac.draw();
		dc.calcTime();
		dc.draw();
		
	}

}
