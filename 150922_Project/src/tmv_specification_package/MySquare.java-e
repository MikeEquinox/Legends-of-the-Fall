package tmv_specification_package;

public class MySquare extends Methods {
	
	private int x;
	private int y;
	private int side;

	MySquare(int x, int y, int side) {
		this.x = x;
		this.y = y;
		this.side = side;
		System.out.println("Constructor square: "+ x + " " + y + " " + side);
	}
	
	@Override
	void draw() {
		System.out.println("Drawing square");
	}
	
	@Override
	void fill() {
		System.out.println("Filling square");
	}
}
